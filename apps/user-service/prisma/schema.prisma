generator client {
  provider = "prisma-client-js"
    output   = "../src/generated/client"

}

datasource db {
  provider = "postgresql"
}

model User {
  id          String   @id @default(uuid())
  email       String   @unique
  displayName String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("users")
}